{
  "name": "ethers-api",
  "version": "0.0.0",
  "description": "a api for ethereum",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "ts-node": "node --max_old_space_size=4096 --experimental-specifier-resolution=node --loader ts-node/esm",
    "build": "tsc",
    "start": "yarn ts-node --require dotenv/config index.ts",
    "truffle:compile": "truffle compile --config truffle-config.cjs",
    "truffle:migrate": "truffle migrate --reset --config truffle-config.cjs",
    "test": "node --experimental-vm-modules node_modules/.bin/jest  --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "github.com/damingerdai/ethers-api"
  },
  "keywords": [
    "ethers",
    "nodejs",
    "typescript",
    "ethereum"
  ],
  "author": "arthur ming",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.1",
    "ethers": "^5.6.9"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "crypto-random-string": "^5.0.0",
    "jest": "^28.1.2",
    "solc": "^0.8.15",
    "truffle": "^5.5.21",
    "ts-jest": "^28.0.5",
    "ts-node": "10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
